	"require": {

    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "league/flysystem-ziparchive": "^1.0.2",
        "monolog/monolog": "1.*",
        "luminous/luminous": "^0.7.0",
